{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "format": "prettier --write '**/*.{js,ts,tsx,html,json,md}'",
    "clean": "rimraf .next"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,html,json,md}": [
      "npm run format",
      "git add"
    ],
    "**/*.{js,ts,tsx}": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "gray-matter": "^4.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "moment": "^2.29.1",
    "next": "^9.5.5",
    "next-optimized-images": "^2.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-images": "^1.1.7",
    "react-photo-gallery": "^8.0.0",
    "remark": "^12.0.1",
    "remark-html": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.5",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "next-compose-plugins": "^2.2.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  }
}
